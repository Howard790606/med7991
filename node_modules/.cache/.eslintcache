ex % 2 == 0){\n                this.setState({AFscore: this.state.AFscore+1})\n            }\n            else{\n                this.setState({AFscore: this.state.AFscore-1})\n            }\n        };\n\n    render() {\n        return (\n            <div>\n                <h2><font color=\"#FF5959\">CHA<sub>2</sub>DS<sub>2</sub>VASC Score for Atrial fibrillation</font></h2>\n                <input type=\"checkbox\" value=\"age\" onChange={this.changeCHF} /> Congestive heart failure<a>&nbsp;&nbsp;</a>\n                <input type=\"checkbox\" value=\"Risk\" onChange={this.changeAge}/> Age>=75 yr (+2)<a>&nbsp;&nbsp;</a>\n                <input type=\"checkbox\" value=\"knownCAD\" onChange={this.changeDM}/> Diabetes mellitus<a>&nbsp;&nbsp;</a>\n                <br></br>\n                <input type=\"checkbox\" value=\"ASA\" onChange={this.changeStroke}/> Stroke/TIA<a>&nbsp;&nbsp;</a>\n                <input type=\"checkbox\" value=\"ServereAngina\" onChange={this.changeVascular}/> Vascualr disease(MI, PAD, Ao plaque)<a>&nbsp;&nbsp;</a>\n                <input type=\"checkbox\" value=\"EKGST\" onChange={this.changeAge2}/> Age 65~74 yr (+1)<a>&nbsp;&nbsp;</a>\n                <input type=\"checkbox\" value=\"PosCard\" onChange={this.changeSex}/> Sex (female)<a>&nbsp;&nbsp;</a>\n                <h3>Score: {this.state.AFscore}</h3>\n            </div>\n        );\n    }\n}\n","/Users/howard790606/Downloads/med7991/src/containers/Main/Nephro/CCr.js",["166","167","168","169","170","171","172"],"import React, { Component } from \"react\";\nimport { NavLink } from \"react-router-dom\";\n\nexport default class CCr extends Component {\n        constructor(props) {\n          super(props);\n          this.state = { \n            ccr: 0,\n            eGFR: 0,\n            kgValue : 0,\n            ageValue : 0,\n            creValue : 0,\n            gender: \"male\"\n         };\n        }\n\n    handleIncCCr = () => {\n        if(this.state.gender==\"male\"){\n            thi